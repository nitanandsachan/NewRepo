namespace Examples.Expressions.Eval.LINQ_Dynamic.Aggregate_Operators
{
    public partial class Aggregate : Form
    {
        public Aggregate()
        {
            InitializeComponent();
        }
        private void uiAggregate_Simple_LINQ_Click(object sender, EventArgs e)
        {
            string[] categories = new string[] { "Beverages", "Condiments", "Vegetables", "Dairy Products", "Seafood" };
            var products = My.GetProductList();

            var q = from c in categories join p in products on c equals p.Category select new { Category = c, p.ProductName };

            foreach (var v in q)
            {
                Console.WriteLine(v.ProductName + ": " + v.Category);
            }
        }

        private void uiAggregate_Simple_LINQ_Dynamic_Click(object sender, EventArgs e)
        {

